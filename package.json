    "qs": "^6.14.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^3.6.2",
    "rollup": "^4.53.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0"
