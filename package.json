{
  "name": "rhoto-router",
  "version": "0.1.6",
  "description": "a react router",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": "github.com/jbr/rhoto-router",
  "types": "dist/index.d.ts",
