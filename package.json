    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.17.0",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
